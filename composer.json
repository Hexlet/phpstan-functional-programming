{
    "name": "hexlet/phpstan-fp",
    "description": "PHPStan rules for functional programming",
    "type": "library",
    "require": {
        "php": "^7.1",
        "phpstan/phpstan": "^0.12.48"
    },
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
          "Hexlet\\PHPStanFp\\": "src/",
          "Hexlet\\PHPStanFp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "extension.neon"
            ]
        }
    }
}
